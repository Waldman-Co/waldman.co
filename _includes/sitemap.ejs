<?xml version="1.0" encoding="UTF-8"?>
<!-- <?xml-stylesheet type="text/xml" href="/css/sitemap-template.xsl"?> -->
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" >
    <% for (let page of collections.page) { %>
        <url>
            <loc>https://waldman.co<%= page.url %></loc>
            <priority>1</priority>
            <% if(page.url.length === 1) { %>
                <image>
                    <loc>https://waldman.co/<%= page.data.firstHero %></loc>
                </image>
            <% } %>
        </url>
    <% } %>
    <% for (let post in collections.post){ %>
        <url>
            <loc>https://waldman.co/posts/<%= Number(post) + 1 %></loc>
            <lastmod><%= collections.post[post].date %></lastmod>
            <priority>0.7</priority>
        </url>
    <% } %>
<%= content %>
</urlset>
